<div class="container theme-showcase" role="main">
  <div class="jumbotron">
    <h1>DingoDX Public Data Validation</h1>
    <p>These example properties are intended to provide a mapping of data which is considered to be in the public domain from sources external to DingoDX.</p>
  </div>

  <% propertyData.forEach( function( property ){ %>
  <div class="list-group" vocab="http://schema.org/" typeof="Place">
    <div class="list-group-item active">
      <h4 class="list-group-item-heading"><%= property.name %></h4>
    </div>
    <div class="list-group-item">
      <h4 class="list-group-item-heading">Property UUID</h4>
      <p class="list-group-item-text"><%= property.propertyuuid %></p>
    </div>
    <div class="list-group-item">
      <h4 class="list-group-item-heading">Date Property Created</h4>
      <p class="list-group-item-text"><%= property.datepropertycreated %></p>
    </div>
    <div class="list-group-item">
      <h4 class="list-group-item-heading">Version UUID</h4>
      <p class="list-group-item-text"><%= property.versionuuid %></p>
    </div>
    <div class="list-group-item">
      <h4 class="list-group-item-heading">Date Version Created</h4>
      <p class="list-group-item-text"><%= property.dateversioncreated %></p>
    </div>
    <div class="list-group-item">
      <h4 class="list-group-item-heading">Description</h4>
      <p class="list-group-item-text" property="description"><%= property.description %></p>
    </div>
    <div class="list-group-item">
      <h4 class="list-group-item-heading">Image</h4>
      <p class="list-group-item-text">
        <img class='img-responsive' property="image" src='<%= property.image %>'></img>
      </p>
    </div>
    <div class="list-group-item" property="geo.GeoCoordinates">
      <h4 class="list-group-item-heading">Geolocation</h4>
      <p class="list-group-item-text">
        <span>Latitude: </span><span property="latitude" ><%= property.geo.latitude %></span>
      </p>
      <p class="list-group-item-text">
        <span>Longitude: </span><span property="longitude" ><%= property.geo.longitude %></span>
      </p>
    </div>
    <div class="list-group-item">
      <h4 class="list-group-item-heading">Google Place Identifier</h4>
      <p class="list-group-item-text"><%= property.googleplaceid %></p>
    </div>
  </div>
  <% }); %>


